{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "right",
            "segments": [
                {
                    "type": "spotify",
                    "style": "plain",
                    "foreground": "#f8fafc",
                    "properties": {
                      "playing_icon": "\uE602 ",
                      "paused_icon": "\uF8E3 ",
                      "stopped_icon": "\uF04D "
                    }
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "background": "#cb997e",
                    "foreground": "#000000",
                    "style": "plain",
                    "template": " {{ .UserName }}@{{ .HostName }}/",
                    "type": "session"
                },
                {
                    "background": "#cb997e",
                    "foreground": "#000000",
                    "properties": {
                        "style": "folder"
                    },
                    "style": "plain",
                    "template": "{{ .Path }}/ ",
                    "type": "path"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [
                {
                    "foreground": "#f8fafc",
                    "properties": {
                        "style": "roundrock",
                        "threshold": 0
                    },
                    "style": "diamond",
                    "template": " {{ .FormattedMs }} ",
                    "type": "executiontime"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "background": "#ddbea9",
                    "foreground": "#000000",
                    "properties": {
                        "cherry_pick_icon": "✓ ",
                        "commit_icon": "▷ ",
                        "fetch_status": true,
                        "merge_icon": "◴ ",
                        "no_commits_icon": "[no commits]",
                        "rebase_icon": "Ɫ ",
                        "tag_icon": "▶ "
                    },
                    "style": "plain",
                    "template": " {{ .HEAD }}{{ if and (eq .Ahead 0) (eq .Behind 0) }} ≡{{end}}{{ if gt .Ahead 0 }} ↑{{.Ahead}}{{end}}{{ if gt .Behind 0 }} ↓{{.Behind}}{{end}} {{ if .Working.Changed }}+{{ .Working.Added }} ~{{ .Working.Modified }} -{{ .Working.Deleted }} {{ end }} ",
                    "type": "git"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [
                {
                    "background": "#502eca",
                    "foreground": "#ffffff",
                    "properties": {
                        "fetch_version": true
                    },
                    "style": "plain",
                    "template": " \uE77F {{ .Full }} ",
                    "type": "dotnet"
                },
                {
                    "background": "#8bbf45",
                    "foreground": "#ffffff",
                    "properties": {
                        "fetch_version": true
                    },
                    "style": "plain",
                    "template": " \uE718 {{ .Full }} ",
                    "type": "node"
                },
                {
                    "background": "#c40011",
                    "foreground": "#ffffff",
                    "properties": {
                        "fetch_version": true
                    },
                    "style": "plain",
                    "template": " \ue71e {{ .Full }} ",
                    "type": "npm"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#ffe8d6",
                    "properties": {
                        "time_format": "January 2, 2006 @ 15:04:05"
                    },
                    "style": "plain",
                    "template": "{{ .CurrentDate | date .Format }}:",
                    "type": "time"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 2
}
